    "print(val)\n",
    "s=[\"M\",\"CM\",\"D\",\"CD\",\"C\",\"XC\",\"L\",\"XL\",\"X\",\"IX\",\"V\",\"IV\",\"I\"]\n",
    "ans=\" \"\n",
    "i=0\n",
    "while n>0:\n",
    "    for j in range(n//val[i]):\n",
    "        ans=ans+s[i]\n",
    "        n=n-val[i]\n",
    "    i=i+1\n",
    "print(ans)\n"
