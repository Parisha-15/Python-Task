   "source": [
    "#  Palindrome  Number \n",
    "n=int(input(\"Enter number\"))\n",
    "ans=0\n",
    "z=n\n",
    "while n>0:\n",
    "    d=n%10\n",
    "    ans=ans*10+d\n",
    "    n=n//10\n",
    "if z==ans:\n",
    "    print(z,\"is a Palindrome number\")\n",
    "else:\n",
    "    print(z,\"is not a Palindrome number\")\n",
    "    "
   ]
