   "source": [
    "#  Reversed number Eg: 123 ==> 321\n",
    "n=int(input(\"Enter number\"))\n",
    "ans=0\n",
    "z=n\n",
    "while n>0:\n",
    "    d=n%10\n",
    "    ans=ans*10+d\n",
    "    n=n//10\n",
    "print(\"Reversed number is\",z,\"is:\",ans)"
   ]
