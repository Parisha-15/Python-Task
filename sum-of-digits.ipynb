    "# Sum of digits of a given number \n",
    "# eg 123 ans : 6 ==> 1+2+3=6\n",
    "n=int(input(\"Enter number\"))\n",
    "ans=0\n",
    "z=n\n",
    "while n>0:\n",
    "    d=n%10\n",
    "    ans=ans+d\n",
    "    n=n//10\n",
    "print(\"sum of digits of\",z,\"is:\",ans)"
   ]
